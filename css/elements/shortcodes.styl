/*----------------------*/ 
/*   CAPTION IMAGE      */ 
/*----------------------*/

.img-with-caption
	position relative
	margin-bottom 30px
	.caption
		position absolute
		top 0
		left 0
		margin 15px
		opacity 0
		padding 4px 20px
		background-color $lwhite
		letter-spacing 2px
		font-size 12px
		text-transform uppercase
		color $black
		transition all .2s ease-in-out
		transform scaleY(0)
		transform-origin bottom
	&:hover .caption
		transform scaleY(1)
		opacity 1

.img-with-caption:last-child
	margin-bottom 0

/*----------------------*/ 
/*   ALERT              */ 
/*----------------------*/

.alert
	border-radius 0
	text-transform uppercase
	font-size 13px
	letter-spacing 1px


/*----------------------*/ 
/*   TABS               */ 
/*----------------------*/

.nav-tabs
	border-bottom none
	
.nav-tabs .nav-link
	margin-bottom 15px
	padding 15px 20px
	font-size 13px
	letter-spacing 1px
	text-transform uppercase
	border-radius 0
	border none
	background-color $lgray
	position relative
	color $text
	&:focus
		background-color $lgray
	&:not(.active):hover
		color $black
	&.active, &.active:hover, &.active:focus
		background-color $black
		color $lwhite
		&::after
			content ''
			position absolute
			bottom -6px
			left 0
			width 0
			height 0
			border-left 12px solid $lblack
			border-bottom 6px solid transparent
			animation tabCur .4s forwards

.tab-content .tab-pane
	padding 0 15px 20px 15px

@keyframes tabCur
	0%
		transform translateY(-100%)
		opacity 0
	100%
		transform translateY(0)
		opacity 1

/*----------------------*/ 
/*   ACCORDION          */ 
/*----------------------*/

.panel-title a, .panel-title a:focus
	position relative
	display block
	padding 15px 65px 15px 20px
	margin-bottom 15px
	font-size 13px
	text-transform uppercase
	letter-spacing 1px
	background-color $lgray
	transition $transition
	color $text
	&::before
		position absolute
		top 0
		right 0
		bottom 0
		width 55px
		border-left 2px solid $white
		font-size 18px
		line-height 55px
		text-align center
		background-color $lgray
		font-family 'FontAwesome'
		content '\f107'
		transition $transition

.panel-title a[aria-expanded="true"]:before
	content '\f106'
	background-color $black
	color $lwhite

.panel-title a:hover
	color $black
	 
.panel-title a[aria-expanded="true"]
	background-color $black
	color $lwhite
	
.panel-group .panel-heading + .panel-collapse > .list-group, .panel-group .panel-heading + .panel-collapse > .panel-body
	padding 0 15px 20px 15px
	

/*----------------------*/ 
/*   PROGRESS BAR       */ 
/*----------------------*/

.progress-bar
	position relative
	margin 0 0 20px 0
	h6
		width 40%
		float left
		
.progress-outer
	height 1px
	display inline-block
	vertical-align middle
	position relative
	width 60%
	background-color $transparent
	.progress-inner
		height 1px
		float left
		width 50px
		background-color $black
		
.progress-inner > span
	position relative
	float right
	left 40px
	top -14px
	color $black
	&::after
		content '%'
		margin-left 2px

@media (max-width: 768px)
	.progress-bar
		h6
			float none

/*----------------------*/ 
/*   OVERLAY            */ 
/*----------------------*/

.vl-overlay-dark
	position relative
	&::before
		content ''
		position absolute
		top 0
		left 0
		width 100%
		height 100%
		background-color rgba($black, 90%)
		z-index -1
	~ div
		position relative
		z-index 2

/*----------------------*/ 
/*   OUR TEAM           */ 
/*----------------------*/

.team-item
	position relative
	&::after
		content ''
		position absolute
		top 0
		left 0
		height 100%
		width 100%
		background-color rgba($black, 95%)
		z-index 1
		opacity 0
		transition all .25s ease-in-out
	&:hover::after
		opacity 1

.team-name,
.team-function
	transition $transition
	white-space nowrap
	webkit-font-smoothing antialiased
	-khtml-font-smoothing antialiased
	-apple-font-smoothing antialiased

.team-name,
.team-function
	opacity 0

.team-content
	position absolute
	z-index 2
	text-align center
	padding 30px
	left 0
	top 50%
	width 100%
	transform translateY(-50%)
	
.team-name
	text-transform uppercase
	letter-spacing 2px
	color $white
	transform translateY(-8px)
	
.team-function
	transform translateY(8px)
	font-size 12px
	color $lgray
	font-style italic
	letter-spacing 2px
	
.team-name, 
.team-function
	opacity 0

.team-item:hover 
	.team-name, .team-function
		opacity 1
		transform translateY(0)
		
@media (max-width: 544px)
	.team-item
		margin-bottom 30px

/*----------------------*/ 
/*   PARTNER / CLIENT   */ 
/*----------------------*/

.logo-wrap
	display table
	width 100%
	height 150px
	> a
		display table-cell
		vertical-align middle
		text-align center
		opacity .8
		transition all .3s ease-in-out
		&:hover
			opacity 1
		img
			max-height 65px
			display inline-block
			

/*----------------------*/ 
/*   SOCIAL LINKS       */ 
/*----------------------*/

.social-link
	margin 0 0 20px 0
	padding 0
	list-style-type none
	&::after
		content ''
		display table
		clear both
	li
		float left
		a
			display block
			height 22px
			width 22px
			border-radius 2px
			font-size 11px
			line-height 22px
			text-align center
			color $white
			background-color $black
	li + li
		margin-left 8px


/*----------------------*/ 
/*   BUTTONS            */ 
/*----------------------*/

.btn
	display inline-block
	text-align center
	text-transform uppercase
	border-radius 0px
	box-shadow none
	letter-spacing 1px
	font-size 12px
	outline none
	transition $transition
	i
		margin-left 5px
	&.disable
		cursor default
	&:focus,
	&:active
		outline none

.btn-white
	border 1px solid $lwhite
	color $black
	background-color $lwhite
	&:hover
		color $lwhite
		background-color transparent
		
.btn-black
	border 1px solid $lblack
	color $white
	background-color $black
	&:hover
		color $black
		background-color transparent
		
.btn-border-white
	border 1px solid $lwhite
	color $white
	background-color transparent
	&:hover
		color $black
		background-color $lwhite
		
.btn-border-black
	border 1px solid $lblack
	color $black
	background-color transparent
	&:hover
		color $lwhite
		background-color $black

.link
	font-weight 700
	letter-spacing 0.1em
	font-size 13px
	color $black
	text-transform uppercase
	&:hover
		opacity .8
	
.btn-reply
	background-color $lgray
	color $text
	&:hover
		color $black

// Size Btn

.btn-xs
	font-size 10px
	padding 5px 15px

.btn-sm
	padding 5px 20px

.btn-md
	padding 10px 35px
			
.btn-lg
	padding 15px 40px

// Animate Icon

@keyframes animateIconTop
	49%
		transform translateY(-100%)
	50%
		opacity 0
		transform translateY(100%)
	51%
		opacity 1

@keyframes animateIconBottom
	49%
		transform translateY(100%)
	50%
		opacity 0
		transform translateY(-100%)
	51%
		opacity 1

@keyframes animateIconLeft
	49%
		transform translateX(-100%)
	50%
		opacity 0
		transform translateX(100%)
	51%
		opacity 1
		
@keyframes animateIconRight
	49%
		transform translateX(100%)
	50%
		opacity 0
		transform translateX(-100%)
	51%
		opacity 1
		
.btn-animateLeft
	&:hover i
		animation animateIconLeft .4s forwards
		
.btn-animateRight
	&:hover i
		animation animateIconRight .4s forwards
		
.btn-animateTop
	&:hover i
		animation animateIconTop .4s forwards
		
.btn-animateBottom
	&:hover i
		animation animateIconBottom .4s forwards

/*----------------------*/ 
/*   CONTACT FORM       */ 
/*----------------------*/

.return-msg
	margin 0
	
.return-msg.show-return-msg
	display block
	text-align center
	letter-spacing 3px
	font-size 12px
	font-weight 700
	text-transform uppercase
	padding 10px
	background-color $lwhite
	color $red
	margin-bottom 20px

/*----------------------*/ 
/*   PRICING TABLE      */ 
/*----------------------*/

.pricing-table
	padding 30px
	text-align center
	border 1px solid $black
	transition border .15s ease
	
.price-header, .price-footer
	padding 10px 0
		
.price-header 
	span
		display block
	i
		font-size 36px
		display inline-block
		color $black

.price-name
	font-size 18px
	text-transform uppercase
	letter-spacing 3px
	padding-left 3px
	margin 10px 0 5px
	padding-bottom 10px
	position relative
	&::after
		content ''
		position absolute
		bottom 0
		left 50%
		width 30px
		margin-left -15px
		height 1px
		background-color $border

.price
	font-size 44px
	line-height 1em
	margin-top 15px
	margin-bottom 5px
	font-weight 600
	
.price-body
	padding 20px 0
	ul
		list-style-type none
		margin 0
		padding 0
		li
			line-height 45px
			border-bottom 1px solid $border
			&:last-child
				border none


@media (max-width: 544px)
	.pricing-table
		margin-bottom 30px

/*----------------------*/ 
/*   TESTIMONIALS       */ 
/*----------------------*/

.testimonial-wrap
	color $white
	text-align center
	padding 70px 0

.testimonial-carousel
	overflow hidden

.testimonial-content
	color $white
	font-family $f2
	margin 0
	padding 0
	border none

.testimonial-title
	h5
		text-transform uppercase
		font-size 12px
		color $white
		letter-spacing 3px
		margin-top 15px
	i
		font-size 42px
		color $white
		display block
		margin 30px 0

.testimonial-author
	margin-top 20px
	text-transform uppercase
	display block
	font-size 10px
	letter-spacing 2px
	color $lgray

.testimonial-pagination
	margin-top 30px


/*----------------------*/ 
/*   SERVICE BOX        */ 
/*----------------------*/

.service-box
	margin-bottom 20px
	.icon-holder
		float left
		margin-right 15px
		margin-bottom 15px
		text-align center
		padding 10px
		min-width 45px
		i
			font-size 24px
			color $black
		h5
			margin-bottom 10px
	.content-holder
		padding-left 60px
	

/*----------------------*/ 
/*   COUNTERUP          */ 
/*----------------------*/

.counter-item
	margin-bottom 20px
	.end-value
		margin-bottom 5px
		font-size 46px
		color $black
	i
		font-style normal


/*----------------------*/ 
/*   GOOGLE MAP         */ 
/*----------------------*/

.map-wrap
	position relative
	overflow hidden

.map-content-wrap
	position absolute
	bottom 0
	left 0
	height 100%
	width 100%
	cursor pointer
	-webkit-user-select none
	-moz-user-select none
	-ms-user-select none
	background-color rgba($black, 90%)
	transition all .3s ease-in-out
	.map-content
		position absolute
		top 50%
		transform translateY(-50%)
		line-height 1em
		left 0
		right 0
		font-size 18px
		text-align center
		color $white
		
.close-map
	display none

.map-open
	.map-content-wrap
		height 50px
	.open-map
		display none
	.close-map
		display block
		

